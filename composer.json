{
    "name": "matthiasnoback/lazy-services-bundle",
    "type": "symfony-bundle",
    "description": "Bundle for marking services and arguments as \"lazy\"",
    "keywords": ["lazy","service","proxy"],
    "homepage": "http://github.com/matthiasnoback/LazyServicesBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthias Noback",
            "email": "matthiasnoback@gmail.com",
            "homepage": "http://php-and-symfony.matthiasnoback.nl"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.3",
        "symfony/proxy-manager-bridge": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "target-dir": "Matthias/LazyServicesBundle",
    "minimum-stability": "dev",
    "autoload": {
        "psr-0" : { "Matthias\\LazyServicesBundle" : "" }
    }
}
